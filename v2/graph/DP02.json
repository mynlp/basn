    "variables": {
        "IDP": {
            "type": "Case",
            "text": "to introduce the death penalty"
        },
        "FAU": {
            "type": "Thing(s)",
            "text": "the Fifth Amendment to the United States Constitution"
        },
        "NDL": {
            "type": "Proposition",
            "text": "No one shall be deprived of life, liberty or property without due process of law."
        },
        "CDL": {
            "type": "Proposition",
            "text": "Citizens might be denied their life, liberty, or property if certain procedural safeguards were in place."
        },
        "DVN": {
            "type": "Proposition",
            "text": "To introduce the death penalty volates the idea that no one shall be deprived of life, liberty or property without due process of law."
        },
        "EFD": {
            "type": "Proposition",
            "text": "Execution under the Federal Death Penalty Act cuts off the opportunity for exoneration."
        },
        "EAU": {
            "type": "Thing(s)",
            "text": "the Eighth Amendment to the United States Constitution"
        },
        "EBR": {
            "type": "Proposition",
            "text": "Excessive bail shall not be required, nor excessive fines imposed, nor cruel and unusual punishments inflicted."
        },
        "DVE": {
            "type": "Proposition",
            "text": "To introduce the death penalty violates the idea that excessive bail shall not be required, nor excessive fines imposed, nor cruel and unusual punishments inflicted."
        },
        "DPC": {
            "type": "Proposition",
            "text": "The death penalty is cruel and unusual."
        },
        "MSA": {
            "type": "Proposition",
            "text": "Many states have adopted a method of execution believed to be the most humane available."
        }
